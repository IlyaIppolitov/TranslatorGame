<Window x:Class="TranslatorGame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TranslatorGame"
        mc:Ignorable="d"
        Title="TranslatorGame" Height="650" Width="900" Loaded="Window_Loaded">

    <StackPanel>
        <DataGrid x:Name="dic" SelectionChanged="dic_SelectionChanged">
            
        </DataGrid>
        <TextBlock Text="Русское название"/>
        <TextBox x:Name="rus"/>
        <TextBlock Text="Английское название"/>
        <TextBox x:Name="eng"/>
        <TextBlock Text="Немецкое название"/>
        <TextBox x:Name="ger"/>

        <Button Click="Button_Click">Категория</Button>
    </StackPanel>
</Window>
